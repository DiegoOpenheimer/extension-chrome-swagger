{
    "manifest_version": 3,
    "name": "Open swagger session",
    "description": "Open all swagger session",
    "version": "1.0",
    "author": "Diego Alves Openheimer",
    "icons": {
        "128": "128.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": ["tabs", "scripting"],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}